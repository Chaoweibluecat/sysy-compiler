global @a = alloc [[i32, 200], 2], zeroinit

decl @getint(): i32

decl @getch(): i32

decl @getarray(*i32): i32

decl @putint(i32)

decl @putch(i32)

decl @putarray(i32, *i32)

decl @starttime()

decl @stoptime()

fun @main(): i32 {
%entry:
  %0 = getelemptr @a, 1
  %1 = getelemptr %0, 1
  %2 = getelemptr @a, 0
  %3 = getelemptr %2, 0
  %4 = load %3
  store %4, %1
  ret 1
}
