fun @main(): i32 {
%entry:
  @a = alloc i32
  store 1, @a
  jump %while-entry

%while-entry:
  %0 = load @a
  %1 = lt %0, 10
  br %1, %while_body, %while-end

%while_body:
  %2 = load @a
  %3 = lt %2, 1
  br %3, %then, %else

%while-end:
  ret 0

%then:
  %4 = load @a
  %5 = add %4, 2
  store %5, @a
  jump %if-end

%else:
  %6 = load @a
  %7 = add %6, 1
  store %7, @a
  jump %while-entry
  jump %if-end

%if-end:
  jump %while-entry
}
