global @var = alloc i32, zeroinit

decl @getint(): i32

decl @getch(): i32

decl @getarray(*i32): i32

decl @putint(i32)

decl @putch(i32)

decl @putarray(*i32, i32)

decl @starttime()

decl @stoptime()

fun @test(): i32 {
%entry:
  @one = alloc i32
  store 1, @one
  %0 = load @one
  ret %0
}

fun @main(): i32 {
%entry:
  %1 = load @var
  %2 = add %1, 1
  %3 = call @test()
  %4 = add %2, %3
  ret %4
}
