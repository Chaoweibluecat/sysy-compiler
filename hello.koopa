decl @getint(): i32

decl @getch(): i32

decl @getarray(*i32): i32

decl @putint(i32)

decl @putch(i32)

decl @putarray(i32, *i32)

decl @starttime()

decl @stoptime()

fun @f1d(@arr: *i32) {
%entry:
  %0 = alloc *i32
  store @arr, %0
  ret
}

fun @f2d(@arr: *[i32, 10]) {
%entry:
  %1 = alloc *[i32, 10]
  store @arr, %1
  %2 = load %1
  %3 = getptr %2, 0
  %4 = getelemptr %3, 0
  call @f1d(%4)
  ret
}

