decl @getint(): i32

decl @getch(): i32

decl @getarray(*i32): i32

decl @putint(i32)

decl @putch(i32)

decl @putarray(*i32, i32)

decl @starttime()

decl @stoptime()

fun @f(@arr: *i32): i32 {
%entry:
  %0 = alloc *i32
  store @arr, %0
  %1 = load %0
  %2 = getptr %1, 1
  %3 = load %2
  ret %3
}

fun @main(): i32 {
%entry:
  %4 = alloc [i32, 2]
  %5 = getelemptr %4, 0
  store 1, %5
  %6 = getelemptr %4, 1
  store 2, %6
  %7 = getelemptr %4, 0
  %8 = call @f(%7)
  ret %8
}
