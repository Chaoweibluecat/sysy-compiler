fun @main(): i32 {
%entry:
  %0 = ne 1, 0
  %1 = alloc i32
  br %0, %then_block, %else

%then_block:
  store 1, %1
  jump %ifend

%else:
  %2 = eq 2, 0
  %3 = alloc i32
  br %2, %then_block_0, %else_0

%ifend:
  %4 = load %1
  @a = alloc i32
  store %4, @a
  %5 = load @a
  ret %5

%then_block_0:
  store 0, %3
  jump %ifend_0

%else_0:
  %6 = ne 3, 0
  store %6, %3
  jump %ifend_0

%ifend_0:
  %7 = load %3
  %8 = ne %7, 0
  store %8, %1
  jump %ifend
}
