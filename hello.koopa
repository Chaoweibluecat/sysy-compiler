fun @main(): i32 {
%entry:
  @a = alloc i32
  store 0, @a
  @b = alloc i32
  store 1, @b
  %0 = load @a
  %1 = eq %0, 0
  %2 = alloc i32
  br %1, %then_block, %else

%then_block:
  store 0, %2
  jump %if-end

%else:
  %3 = load @b
  %4 = ne %3, 0
  store %4, %2
  jump %if-end

%if-end:
  %5 = load %2
  ret %5
}
